  "name": "fate_srd-frontend",
  "version": "1.0.0-beta.3",
  "description": "Storybook development + Webpack Build + Drupal 8 theme",
  "keywords": [
    "component library",
    "design system",
    "drupal",
    "pattern library",
    "storybook",
    "styleguide"
  ],
  "author": "Evan Willhite <evan@fourkitchens.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.15.8",
